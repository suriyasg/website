---
interface Props {
	date: string;
	Time1: string;
	subject1: string;
	Time2: string;
	subject2: string;
}

const { date, Time1, Time2, subject1, subject2 } = Astro.props;
---

<div class="bg-violet-200 rounded-2xl px-7 py-1">
	<h4>{date}</h4>
	<p>{Time1}<br /> {subject1}</p>
	<p>{Time2}<br /> {subject2}</p>
</div>
